		reg = <0x2D>;                /* 1 bit right shift */
		id = <3>;                    /* matching fimc_is_sensor id */
		sensor_f_number = <220>;     /* f number 2.2 */
		setfile = "setB";
	};

};

/* PERIC1 CAM2 */
&hsi2c_2 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpc0 4 0 &gpc0 5 0>;
	status = "okay";
	clock-frequency = <400000>;
	samsung,reset-before-trans;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c2_bus_in>;
	pinctrl-1 = <&hsi2c2_bus>;
	pinctrl-2 = <&hsi2c2_bus_in>;

	fimc-is-imx616@34 {
		compatible = "samsung,exynos5-fimc-is-cis-imx616";
		reg = <0x1A>;               /* 1 bit right shift */
		id = <1>;                   /* matching fimc_is_sensor id */
		sensor_f_number = <220>;    /* f number 2.2 */
		setfile = "setB";
		use_initial_ae;             /* for initial AE of face unlock */
	};

	fimc-is-gc5035@6E {
		compatible = "samsung,exynos5-fimc-is-cis-gc5035";
		reg = <0x37>;                /* 1 bit right shift */
		id = <2>;                    /* matching fimc_is_sensor id */
		sensor_bayer_order = <1>;    /* RGGB (Using setA) */
		sensor_f_number = <240>;     /* f number 2.4 */
		/* setfile = "setA"; */      /* Setfile is selected by sensor_bayer_order */
		use_initial_ae;              /* for initial AE of bokeh */
	};
	
	fimc-is-macro-gc5035@7E {
		compatible = "samsung,exynos5-fimc-is-cis-gc5035";
		reg = <0x3F>;                /* 1 bit right shift */
		id = <4>;                    /* matching fimc_is_sensor id */
		sensor_bayer_order = <1>;    /* RGGB (Using setA) */
		sensor_f_number = <240>;     /* f number 2.4 */
		/* setfile = "setA"; */      /* Setfile is selected by sensor_bayer_order */
	};
};

/* PERI1 CAM3 */
&hsi2c_3 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpc0 6 0 &gpc0 7 0>;
	status = "okay";
	clock-frequency = <400000>;
	samsung,reset-before-trans;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c3_bus_in>;
	pinctrl-1 = <&hsi2c3_bus>;
	pinctrl-2 = <&hsi2c3_bus_in>;

	fimc-is-actuator@18 {
		compatible = "samsung,exynos5-fimc-is-actuator-dw9818";
		reg = <0x0C>;
		id = <0>;                    /* matching sensor id */
	};

	rear_eeprom@B0 {
		compatible = "samsung,rear-eeprom-i2c";
		reg = <0x58>;
		rom_power_position = <0>;
		rom_size = <0x4000>;
		cal_map_es_version = <22>;
		camera_module_es_version = "A";
		/* skip_header_loading; */
		/* skip_crc_check; */
		/* skip_cal_loading; */

		/* M62_Rear_Cal_Map_V022_20201105_for_LSI */
		/* 0 header */
		header_crc_check_list = <0x0000 0x01FB 0x01FC>;
		/* 0 master LSC  */
		/* 1 master all */
		/* 2 Crosstalk cal data */
		/* 3 dual cal */
		crc_check_list = <0x0200 0x15FB 0x15FC
						0x1600 0x283F 0x28FC
						0x2900 0x36DF 0x36FC
						0x3700 0x3F0B 0x3FFC>;

		/* Master */
		rom_header_version_start_addr = <0x50>;
		rom_header_cal_data_start_addr = <0x200>;

		rom_header_cal_map_ver_start_addr = <0x70>;
		rom_header_isp_setfile_ver_start_addr = <0x74>;
		rom_header_project_name_start_addr = <0x7E>;

		rom_header_module_id_addr = <0xAE>;
		rom_header_sensor_id_addr = <0xB8>;
		rom_header_mtf_data_addr = <0xE0>;

		rom_awb_master_addr = <0x2660>;
		rom_awb_module_addr = <0x2684>;

		rom_af_cal_macro_addr = <0x2640>;
		rom_af_cal_d30_addr = <0x2638>;
		rom_af_cal_pan_addr = <0x2634>;

		/* Rear Crosstalk Cal data */
		rom_xtc_cal_data_start_addr = <0x2900>;
		rom_xtc_cal_data_size = <3552>;		/* including checksum */
		rom_xtc_cal_data_addr_list = <0x2900 0x34D4>;
	};

	rear2_eeprom@A8 {	/* Ultra wide eeprom */
		compatible = "samsung,rear2-eeprom-i2c";
		reg = <0x54>;
		rom_power_position = <4>;
		rom_size = <0x2000>;
		cal_map_es_version = <19>;
		camera_module_es_version = "M";
		/* skip_header_loading; */
		/* skip_crc_check; */
		/* skip_cal_loading; */

		/* M62_Rear_Cal_Map_V022_20201105_for_LSI */
		/* 0 header */
		header_crc_check_list = <0x0000 0x00FB 0x00FC>;
		/* 0 master LSC */
		/* 1 master all */
		/* 2 master color */
		crc_check_list = <0x0100 0x1AFB 0x1AFC
						0x1B00 0x1BDF 0x1BFC
						0x1C00 0x1CFB 0x1CFC>;

		/* Master */
		rom_header_version_start_addr = <0x40>;
		rom_header_cal_data_start_addr = <0x100>;

		rom_header_cal_map_ver_start_addr = <0x50>;
		rom_header_isp_setfile_ver_start_addr = <0x54>;
		rom_header_project_name_start_addr = <0x5E>;

		rom_header_module_id_addr = <0xAE>; /* 0xA8 + reserved bytes */
		rom_header_sensor_id_addr = <0xB8>;
		rom_header_mtf_data_addr = <0x66>;

		rom_awb_master_addr = <0x1B00>;
		rom_awb_module_addr = <0x1B24>;
	};

	rear3_eeprom@A4 {	/* Macro wide eeprom */
		compatible = "samsung,rear3-eeprom-i2c";
		reg = <0x52>;
		rom_power_position = <6>;
		rom_size = <0x2000>;
		cal_map_es_version = <22>;
		camera_module_es_version = "A";
		/* skip_header_loading; */
		/* skip_crc_check; */
		/* skip_cal_loading; */

		/*M62_Rear_Cal_Map_V022_20201105_for_LSI*/
		header_crc_check_list = <0x0000 0x00FB 0x00FC>;

		crc_check_list = <0x0100 0x14FB 0x14FC 0x1500 0x16DF 0x16FC>;

		/* Master */
		rom_header_version_start_addr = <0x40>;
		rom_header_cal_data_start_addr = <0x100>;

		rom_header_cal_map_ver_start_addr = <0x50>;
		rom_header_isp_setfile_ver_start_addr = <0x54>;
		rom_header_project_name_start_addr = <0x5E>;

		rom_header_module_id_addr = <0xAE>; /* 0xA8 + reserved bytes */
		rom_header_sensor_id_addr = <0xB8>;

		rom_header_mtf_data_addr = <0x66>;

		rom_awb_master_addr = <0x1530>;
		rom_awb_module_addr = <0x1554>;
	};

	front_eeprom@A2 {
		compatible = "samsung,front-eeprom-i2c";
		reg = <0x51>;
		rom_power_position = <1>;
		rom_size = <0x2000>;
		cal_map_es_version = <12>;
		camera_module_es_version = "A";
		/* skip_header_loading; */
		/* skip_crc_check; */
		/* skip_cal_loading; */

		/* M62_Front_Cal_Map_V010_20201006_for_LSI */
		/* 0 header */
		header_crc_check_list = <0x0000 0x01FB 0x01FC>;
		/* 0 Camera 1 */
		/* 1 Crosstalk Cal Data */
		crc_check_list = <0x0200 0x16DF 0x16FC
						0x1700 0x1EDF 0x1EFC>;

		/* Master */
		rom_header_version_start_addr = <0x30>;
		rom_header_cal_data_start_addr = <0x200>;

		rom_header_cal_map_ver_start_addr = <0x50>;
		/* rom_header_isp_setfile_ver_start_addr = <>; */
		rom_header_project_name_start_addr = <0x5C>;

		rom_header_module_id_addr = <0xAE>; /* 0xA8 + reserved bytes */
		rom_header_sensor_id_addr = <0xB8>;
		rom_header_mtf_data_addr = <0xF0>;

		rom_awb_master_addr = <0x0230>;
		rom_awb_module_addr = <0x023C>;

		/* Front Crosstalk Cal data */
		rom_xtc_cal_data_start_addr = <0x1700>;
		rom_xtc_cal_data_size = <2016>;
		rom_xtc_cal_data_addr_list = <0x1700 0x1D18>;
	};
};

