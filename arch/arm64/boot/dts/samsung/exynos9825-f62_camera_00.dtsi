#include <dt-bindings/camera/fimc_is.h>

/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {
			/* REAR CAMERA */
			fimc_is_sensor_imx682: fimc-is_sensor_imx682@20 {
				compatible = "samsung,sensor-module-imx682";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk0_out>;
				pinctrl-2 = <&sensor_mclk0_fn>;
				pinctrl-3 = <>;

				position = <0>;	/* Rear:0. Front:1. Rear_sub:2. Secure:3. */
				id = <0>;                     /* fimc_is_sensor id */
				mclk_ch = <0>;
				sensor_i2c_ch = <0>; /* SENSOR_CONTROL_I2C0 */

				rom_type = <2>; /* ROM_TYPE_EEPROM */
				rom_id = <0>;
				rom_cal_index = <0>;

				gpio_mclk = <&gpc1 4 0x1>;
				gpio_reset = <&gpg2 6 0x1>; /* sensor reset */
				gpio_ldo_en = <&gpg3 2 0x1>;
				status = "okay";

				af {
					product_name = <27>;     /* ACTUATOR_NAME_DW9818 */
					i2c_addr = <0x18>;
					i2c_ch = <3>;            /* SENSOR_CONTROL_I2C3 */
				};

				flash {
					product_name = <5>; /* FLASH_KTD2692 */
				};

				ois {
					product_name = <100>; /* NOTHING */
				};

				mcu {
					product_name = <100>; /* NOTHING */
				};

				aperture {
					product_name = <100>; /* NOTHING */
				};
			};

			/* REAR Sub CAMERA */
			fimc_is_module_gc5035: fimc-is_sensor_gc5035@6E {
				compatible = "samsung,sensor-module-gc5035";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk2_out>;
				pinctrl-2 = <&sensor_mclk2_fn>;
				pinctrl-3 = <>;

				position = <2>;               /* Rear:0, Front:1, Rear_sub(Rear2):2, Rear_UW(Rear3): 4 */
				id = <2>;                     /* fimc_is_sensor id */
				mclk_ch = <2>;
				sensor_i2c_ch = <2>; /* SENSOR_CONTROL_I2C2 */

				rom_type = <3>; /* ROM_TYPE_OTP */
				rom_id = <0>;
				rom_cal_index = <1>;

				gpio_mclk = <&gpc1 6 0x1>;    /* RCAM2_MCLK */
				gpio_reset = <&gpg3 5 0x1>;   /* RCAM2_RST_N */
				gpio_ldo_en = <&gpg3 4 0x1>;   /* RCAM2_RCAM4_LDO_EN: XGPIO27 - GPG3[4] */
				status = "okay";

			};


			/* REAR Third CAMERA (UW) */
			fimc_is_sensor_3l6: fimc-is_sensor_3l6@5A {
				compatible = "samsung,sensor-module-3l6";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk3_out>;
				pinctrl-2 = <&sensor_mclk3_fn>;
				pinctrl-3 = <>;

				position = <4>;               /* Rear:0, Front:1, Rear_sub(Rear2):2, Rear_UW(Rear3): 4 */
				id = <3>;                     /* fimc_is_sensor id */
				mclk_ch = <3>;

				rom_type = <2>; /* ROM_TYPE_EEPROM */
				rom_id = <2>;
				rom_cal_index = <0>;

				use_avdd_dvdd_share;
				gpio_mclk = <&gpc1 7 0x1>;    /* RCAM3_MCLK */
				gpio_reset = <&gpg0 3 0x1>;   /* RCAM3_RST_N */
				gpio_ldo_en = <&gpg3 3 0x1>;        /* RCAM3_LDO_EN: XGPIO26 - GPG3[3] */
				gpio_subcam_sel = <&gpg4 0 0x1>; /* sub cam sel */
				status = "okay";
			};

			/* REAR Macro CAMERA - REAR Four*/
			fimc_is_macro_module_gc5035: fimc-is_macro_sensor_gc5035@7E {
				compatible = "samsung,sensor-module-gc5035";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk4_out>;
				pinctrl-2 = <&sensor_mclk4_fn>;
				pinctrl-3 = <>;

				position = <6>;               /* Rear:0, Front:1, Rear_sub(Rear2):2, Rear_UW(Rear3): 4, Rear4: 6 */
				id = <4>;                     /* fimc_is_sensor id */
				mclk_ch = <4>;

				rom_type = <2>; /* ROM_TYPE_EEPROM */
				rom_id = <4>;
				rom_cal_index = <0>;

				use_2nd_module;               /* Use GC5035 as MACRO camera sensor*/
				gpio_mclk = <&gpg3 0 0x1>;    /* RCAM4_MCLK */
				gpio_reset = <&gpg3 1 0x1>;   /* RCAM4_RST_N */
				gpio_ldo_en = <&gpg3 4 0x1>;  /* RCAM4_LDO_EN: XGPIO27 - GPG3[4] */
				gpio_subcam_sel = <&gpg4 0 0x1>; /* sub cam sel */

				status = "okay";

				flash {
					product_name = <5>; /* FLASH_KTD2692 */
				};
			};

			fimc_is_flash_ktd2692: fimc-is-flash_ktd2692@0 {
				compatible = "samsung,sensor-flash-ktd2692";
				id = <0 4>; /* matching sensor id */
				status = "okay";
			};

			leds_ktd2692 {
				compatible = "ktd2692";
				flash_control = <&gpg2 3 0x1>;
				max_current = <1500>; /* (IMax) */
				flash_current = <1200>; /* (n/16)xIMax (1<=n<=16) */
				movie_current = <175>; /* (n/16)*IMax/3 (1<=n<=16) */
				factory_current = <175>; /* (n/16)xIMax/3 (1<=n<=16) */
				torch_current = <75>; /* (n/16)xIMax/3 (1<=n<=16) */
				torch_table_enable = <1>;
				torch_table = <1 2 2 3 3 4 4 4 5 5>;
				status = "okay";
				pinctrl-names ="default","host","is";
				pinctrl-0 = <&fimc_is_flash_is>;
				pinctrl-1 = <&fimc_is_flash_host>;
				pinctrl-2 = <&fimc_is_flash_is>;
			};

			/* FRONT CAMERA */
			fimc_is_sensor_imx616: fimc-is_sensor_imx616@34 {
				compatible = "samsung,sensor-module-imx616";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk1_out>;
				pinctrl-2 = <&sensor_mclk1_fn>;
				pinctrl-3 = <>;

				position = <1>;               /* Rear:0, Front:1, Rear_sub(Rear2):2, Rear_UW(Rear3): 4, Rear4: 6 */
				id = <1>;                     /* fimc_is_sensor id */
				mclk_ch = <1>;
				sensor_i2c_ch = <0>;          /* SENSOR_CONTROL_I2C1 */

				rom_type = <2>; /* ROM_TYPE_EEPROM */
				rom_id = <1>;
				rom_cal_index = <0>;
				
				gpio_mclk = <&gpc1 5 0x1>;
				gpio_reset = <&gpg2 5 0x1>;   /* FRONT_RST_N */

				status = "okay";

			};
		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */

/***** reference symbol node *****/

&fimc_is {
	pinctrl-names = "default","sleep";
	pinctrl-0 = <>;
	pinctrl-1 = <&cam_sleep_state &cam_sleep_state2 &cam_sleep_state3>;
	secure_mem_info = <0x96000000 0x2200000>;
	non_secure_mem_info = <0x98200000 0xE00000>;
	vender {
		rear_sensor_id = <124>;     /* SENSOR_NAME_IMX682 */
		front_sensor_id = <121>;    /* SENSOR_NAME_IMX616 */
		rear2_sensor_id = <210>;    /* SENSOR_NAME_GC5035 */
		rear3_sensor_id = <59>;     /* SENSOR_NAME_S5K3L6 */
		rear4_sensor_id = <210>;    /* SENSOR_NAME_GC5035 */
		need_i2c_config;
		use_module_check;
		/* skip_cal_loading; */
		check_sensor_vendor;

		max_supported_camera = <4>;// <6>;
		supported_cameraId = <0 3 50 54>;//<0 3 23 50 52 54>;
		/* REAR, FRONT, DUAL_REAR_PORTRAIT_WIDE(Bokeh), REAR_2, REAR_3, REAR_4(Macro) */

		max_camera_num = <7>;
		camera_info0 {             // 0 : sysfs rear
			internal_id = <0>;     // 0 : wide
			isp = <0>;             /* 0 : INT , 1 : EXT , 2 : SOC */
			cal_memory = <2>;      /* 0 : N , 1 : FROM , 2 : EEPROM , 3 : OTP  */
			read_version = <0>;    /* 0 : SYSFS , 1 : CAMON */
			core_voltage = <0>;    /* 0 : N , 1 : Y  */
			upgrade = <0>;         /* 0 : N , 1 : SYSFS , 2 : CAMON */
			fw_write = <0>;        /* 0 : N,  1 : OS, 2 : SD, 3 : ALL */
			fw_dump = <0>;         /* 0 : N , 1 : Y  */
			companion = <0>;       /* 0 : N , 1 : Y  */
			ois = <0>;             /* 0 : N , 1 : Y  */
			valid = <1>;           /* 0 : INVALID, 1 : VALID */
		};
		camera_info1 {             // 1 : sysfs front
			internal_id = <1>;     // 1 : front master
			isp = <0>;             /* 0 : INT , 1 : EXT , 2 : SOC */
			cal_memory = <2>;      /* 0 : N , 1 : FROM , 2 : EEPROM , 3 : OTP  */
			read_version = <0>;    /* 0 : SYSFS , 1 : CAMON */
			core_voltage = <0>;    /* 0 : N , 1 : Y  */
			upgrade = <0>;         /* 0 : N , 1 : SYSFS , 2 : CAMON */
			fw_write = <0>;        /* 0 : N,  1 : OS, 2 : SD, 3 : ALL */
			fw_dump = <0>;         /* 0 : N , 1 : Y  */
			companion = <0>;       /* 0 : N , 1 : Y  */
			ois = <0>;             /* 0 : N , 1 : Y  */
			valid = <1>;           /* 0 : INVALID, 1 : VALID */
		};
		camera_info2 {             // 2 : sysfs rear2
			internal_id = <4>;     // 4 : ultra wide
			isp = <0>;             /* 0 : INT , 1 : EXT , 2 : SOC */
			cal_memory = <2>;      /* 0 : N , 1 : FROM , 2 : EEPROM , 3 : OTP  */
			read_version = <0>;    /* 0 : SYSFS , 1 : CAMON */
			core_voltage = <0>;    /* 0 : N , 1 : Y  */
			upgrade = <0>;         /* 0 : N , 1 : SYSFS , 2 : CAMON */
			fw_write = <0>;        /* 0 : N,  1 : OS, 2 : SD, 3 : ALL */
			fw_dump = <0>;         /* 0 : N , 1 : Y  */
			companion = <0>;       /* 0 : N , 1 : Y  */
			ois = <0>;             /* 0 : N , 1 : Y  */
			valid = <1>;           /* 0 : INVALID, 1 : VALID */
		};
		camera_info3 {             // 3 : sysfs front2
			valid = <0>;           /* 0 : INVALID , 1 : VALID   */
		};
		camera_info4 {             // 4 : sysfs rear3
			internal_id = <2>;     // 2 : tele
			isp = <0>;             /* 0 : INT , 1 : EXT , 2 : SOC */
			cal_memory = <3>;      /* 0 : N , 1 : FROM , 2 : EEPROM , 3 : OTP  */
			read_version = <0>;    /* 0 : SYSFS , 1 : CAMON */
			core_voltage = <0>;    /* 0 : N , 1 : Y  */
			upgrade = <0>;         /* 0 : N , 1 : SYSFS , 2 : CAMON */
			fw_write = <0>;        /* 0 : N,  1 : OS, 2 : SD, 3 : ALL */
			fw_dump = <0>;         /* 0 : N , 1 : Y  */
			companion = <0>;       /* 0 : N , 1 : Y  */
			ois = <1>;             /* 0 : N , 1 : Y  */
			valid = <1>;           /* 0 : INVALID, 1 : VALID */
		};
		camera_info5 {             // 5 : sysfs front3
			valid = <0>;           /* 0 : INVALID , 1 : VALID   */
		};
		camera_info6 {             // 6 : sysfs rear4
			internal_id = <6>;     // 6 : macro
			isp = <0>;             /* 0 : INT , 1 : EXT , 2 : SOC */
			cal_memory = <2>;      /* 0 : N , 1 : FROM , 2 : EEPROM , 3 : OTP  */
