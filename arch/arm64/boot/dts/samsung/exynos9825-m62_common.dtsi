				cirrus,moisture-pin = <4>;
				cirrus,moisture-imp = <8000>;
				cirrus,moisture-debounce = <100>;

				cirrus,micd-configs = <
					0 1 4 0 1
				>;
				cirrus,hpd-pins = <2 5 0 0>;
			};
		};

		adsps {
			#address-cells = <1>;
			#size-cells = <0>;

			adsp@0FFE00 {
				reg = <0x0FFE00>;
